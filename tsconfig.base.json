{
  "compilerOptions": {
    "composite": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "importHelpers": true,
    "isolatedModules": true,
    "lib": ["es2022"],
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2022",
    "customConditions": ["development"],
    "baseUrl": ".",
    "paths": {
      "@Developer/libs/api-contract/*": ["libs/api-contract/src/*"],
      "@Developer/libs/electric-prisma/*": ["libs/electric-prisma/src/*"]
    }
    // "paths": {
    //   "@Developer/libs/api-contract/*": ["libs/api-contract/*"],
    //   "@Developer/libs/electric-prisma/*": ["libs/electric-prisma/*"]
    // }
  }
}
